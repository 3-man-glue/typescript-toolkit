  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": "git@gitlab.com:sertiscorp/dev/7-11-chat/backend/ts-http-toolkit.git",
  "license": "MIT",
  "files": [
    "cache",
    "ddd",
    "config",
